{
  "$schema": "https://raw.githubusercontent.com/microsoft/vcpkg/master/scripts/vcpkg.schema.json",
  "name": "atlas-engine",
  "version-string": "0.2.0",
  "port-version": 0,
  "builtin-baseline": "5786fcb0cb5eb08d1931a230dad9701e7a6c37f0",
  "homepage": "https://github.com/tippesi/Atlas-Engine",
  "description": "Cross platform toy render engine supporting physically based rendering and software ray tracing",
  "dependencies": [
    {
      "name": "sdl2",
      "default-features": false,
      "features": [
        "vulkan"
      ]
    },
    "glm",
    "stb",
    {
      "name": "imgui",
      "version>=": "1.89.2",
      "features": [
        "docking-experimental"
      ],
      "default-features": false
    },
    "spirv-reflect",
    "assimp",
    "vulkan-memory-allocator",
    "volk",
    "vulkan-headers",
    "glslang",
    "spirv-tools",
    "nlohmann-json",
    "gtest",
    "joltphysics",
    "imguizmo"
  ],
  "overrides": [
    {
      "name": "glm",
      "version": "0.9.8.0"
    },
    {
      "name": "assimp",
      "version": "5.3.1#3"
