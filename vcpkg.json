    {
      "name": "spirv-reflect",
      "version": "1.3.236.0#0"
    },
    {
      "name": "glslang",
      "version": "11.11.0#1"
    },
    {
      "name": "sdl2",
      "version": "2.28.2"
    },
    {
      "name": "vulkan-memory-allocator",
      "version": "3.0.1#3"
    },
    {
      "name": "volk",
      "version": "1.3.250#0"
    },
    {
      "name": "vulkan-headers",
      "version": "1.3.268.0#0"
    },
    {
      "name": "joltphysics",
      "version": "4.0.0"
    }
  ]
}
